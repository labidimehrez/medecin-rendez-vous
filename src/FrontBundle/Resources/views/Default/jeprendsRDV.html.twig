                                <form id="bookingForm" name="bookingForm" method="POST" class="tunnel-form  tunnel-box"  >


                                    <div class="form-group">
                                        <div class="control-label">
                                            Avez-vous déjà consulté ce praticien auparavant&nbsp;?
                                        </div>
                                        <div class="form-group__field">
                                            <label class="form-group__field--radio" for="bookingForm_newPatientTrue">
                                                <input type="radio" id="bookingForm_newPatientTrue" value="true"
                                                       name="newPatient" checked="checked">
                                                <span class="l-radio">
                                                    Je suis un nouveau patient
                                                </span>
                                            </label>
                                            <label class="form-group__field--radio" for="bookingForm_newPatientFalse">
                                                <input type="radio" id="bookingForm_newPatientFalse" value="false"
                                                       name="newPatient">
                                                <span class="l-radio">
                                                    Je suis déjà patient de ce praticien
                                                </span>
                                            </label>
                                        </div>
                                    </div>   
                                
                                    
                                    <div class="form-group f-blue-select">
                                        <label class="control-label" for="bookingForm_actId">
                                            Quelle est la raison de votre visite&nbsp;?
                                        </label>
                                        <div class="form-group__field" id="bookingForm_actIdList_container">
                                            <select class="user__select" id="bookingForm_actId" name="actId"
                                                    onchange="/*$.App.Booking.changeAct('07/06/2017 09:00', 7716);*/">
                                                
                                                {% for raisonvisite in  medecin.Specialite.raisonvisites %}
                                                    <option value="{{raisonvisite.id}}" act-id="{{raisonvisite.id}}" act-type-id="{{raisonvisite.id}}" act-linked-act-id="{{raisonvisite.id}}">
                                                    {{raisonvisite.raison}}
                                                   </option>
                                                
                                                {% endfor %}
                                                
                                            
                                            </select>
                                        </div>
                                    </div>


                                    {% include 'FrontBundle:Default:datetimepicker_include.html.twig' %}
                                    <script type="text/javascript">
                                        $(document).ready(function () {

                                            $("div#step1-actions a#valider").click(function () {
                                                $("div#bookingSelection div.form-group__field p").remove();
                                                $("div#step1-span_choose_rdv span").css({"color": "#787878"});
                                                var selectedDate = $("div#datetimepicker5 input").val();
                                                var selectedDate = selectedDate.trim();

                                                var btns1 = $("form#bookingForm div.form-group__field input:radio").eq(0);
                                                var btns2 = $("form#bookingForm div.form-group__field input:radio").eq(1);
                                                if ($(btns1).is(':checked')) {
                                                    var type = "NOUVELLE CONSULTATION ";
                                                }
                                                else if ($(btns2).is(':checked')) {
                                                    var type = "RENOUVELLEMENT TRAITEMENT";
                                                }

                                                if (selectedDate.length === 0) {
                                                    $("div#step1-span_choose_rdv span").css({"color": "red"});

                                                    return false;
                                                }

                                                else {
                                                    var selectedDate = selectedDate.replace("/", "-");
                                                    var selectedDate = selectedDate.replace("/", "-");
                                                    $.ajax({
                                                        type: 'get',
                                                        datatype: "json",
                                                        url: Routing.generate('valider_RDV', {'date': selectedDate, 'type':type, 'medecinId':{{ medecin.id }}}),
                                                        beforeSend: function () {
                                                            $("div#step1-actions img").show();
                                                        },
                                                        success: function (data) {
                                                            $("div#step1-actions img").hide();
                                                            data = JSON.parse(data);
                                                            console.debug(data);
                                                            if (data.length === 1) {
                                                                $("div#bookingSelection div.form-group__field").append(data);
                                                            }
                                                            else {
                                                                $("div#bookingSelection div.form-group__field").append(data[0]);

                                                                $("div#step1-actions a").eq(0).hide();
                                                                $("div#step1-actions a").eq(1).show();

                                                            }

                                                        }
                                                    });
                                                }

                                            });

                                        });
                                    </script>
                                    <script type="text/javascript">
                                        $(function () {
                                            $('#datetimepicker5').datetimepicker({
                                                defaultDate: "06/05/2017 09:00",
                                                daysOfWeekDisabled: [0, 6]
                                            });
                                        });
                                    </script>
