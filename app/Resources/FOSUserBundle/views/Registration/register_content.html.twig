                        <script type="text/javascript">
                            $(document).ready(function () {
                                $("form :submit").click(function () {

                                    var inputFirst = $("form input:checkbox").eq(0);
                                    var inputSecond = $("form input:checkbox").eq(1);

                                    if ((inputFirst.is(":checked") === true) && (inputSecond.is(":checked") === true)) {
                                        $("form").submit();
                                    }
                                    else {
                                        if ((inputFirst.is(":checked")) === false) {
                                            $("span#inputFirst").css("color", "red");
                                        }
                                        if ((inputSecond.is(":checked")) === false) {
                                            $("span#inputSecond").css("color", "red");
                                        }
                                        return false;
                                    }

                                });
                            });
                        </script>
